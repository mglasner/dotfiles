{
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontSize": 13,
  "editor.fontWeight": "500",
  "editor.fontVariations": true,
  "editor.fontLigatures": true,
  "editor.renderWhitespace": "all",
  "editor.showFoldingControls": "always",
  "editor.renderControlCharacters": true,
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.scrollBeyondLastLine": true,
  "editor.tabSize": 4,
  "editor.wordWrap": "off",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",

  "files.insertFinalNewline": true,
  "files.autoSave": "onFocusChange",
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    "*.log": "plaintext",
    "**/templates/**/*.html": "django-html",
    "*.html": "html"
  },

  "workbench.colorCustomizations": {
    "editorIndentGuide.background1": "#008000",
    "editorIndentGuide.activeBackground1": "#ff0000",
    "tree.indentGuidesStroke": "#05ef3c"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "*.html": "default"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Default High Contrast",
  "workbench.startupEditor": "none",
  "workbench.sideBar.location": "left",
  "workbench.tree.indent": 20,
  "workbench.tree.renderIndentGuides": "always",

  "window.title": "${activeEditorFull}${separator}${rootName}",
  "autoDocstring.docstringFormat": "google",
  "autoDocstring.guessTypes": true,
  "explorer.confirmDelete": false,

  "python.autoComplete.extraPaths": [],
  "python.languageServer": "Pylance",
  "[python]": {
    "editor.rulers": [120],
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },

  "security.workspace.trust.untrustedFiles": "open",

  "errorLens.gutterIconsEnabled": true,
  "errorLens.messageEnabled": false,

  "terminal.integrated.profiles.windows": {
    "Cmder": {
      "path": "cmd.exe",
      "args": ["/k", "C:\\Users\\User\\Google Drive\\Cmder\\vendor\\init.bat"]
    },
    "python-repl": {
      "path": "python",
      "args": ["-q"]
    }
  },
  "terminal.integrated.defaultProfile.windows": "Cmder",
  "terminal.external.windowsExec": "C:\\Users\\User\\Google Drive\\Cmder\\Cmder.exe",
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.env.windows": {},

  "diffEditor.ignoreTrimWhitespace": false,
  "html.format.templating": true,
  "python.analysis.extraPaths": [],
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "[html]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "prettier.endOfLine": "lf",
  "editor.formatOnSave": true,
  "files.exclude": {
    "**/__pycache__": true,
    "**/.git": false,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/.ruff_cache": true,
    "**/.venv": false,
    "**/.vscode": true
  },

  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "explorer.confirmDragAndDrop": false,
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml",
    "editor.formatOnSave": false
  },
  "evenBetterToml.formatter.alignEntries": true,
  "evenBetterToml.formatter.alignComments": true,
  "evenBetterToml.formatter.indentEntries": true,
  "git.openRepositoryInParentFolders": "never",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.completeFunctionParens": true,
  "eslint.validate": ["typescript", "typescriptreact", "javascript", "javascriptreact"],
  "files.eol": "\n",
  "rainbow_csv.csv_lint_detect_trailing_spaces": true,
  "pylint.args": ["disable=all"],
  "editor.mouseWheelZoom": true,
  "console-ninja.featureSet": "Pro",
  "sourcery.codeLens": false,
  "editor.stickyScroll.enabled": false,
  "editor.largeFileOptimizations": false,
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "evenBetterToml.formatter.columnWidth": 80
}
